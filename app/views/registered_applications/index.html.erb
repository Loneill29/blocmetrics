<h1>My Applications</h1>
<div class="col-md-4">
  <%= link_to "Register New Application", new_registered_application_path, class: 'btn btn-success' %>
</div>
<% @registered_applications.each do |registered_application| %>
  <div class="media">
    <div class="media-body">
      <h4 class="media-heading">
        <p><%= link_to registered_application.name, registered_application %></p>
        <% end %>
      </h4>
    </div>
  </div>
